{
    "name": "jaymeh/filament-pages",
    "description": "Functionality for handling basic Page Resources within Filament.",
    "keywords": [
        "jaymeh",
        "laravel",
        "filament-pages"
    ],
    "homepage": "https://github.com/jaymeh/filament-pages",
    "support": {
        "issues": "https://github.com/jaymeh/filament-pages/issues",
        "source": "https://github.com/jaymeh/filament-pages"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Sykes",
            "email": "jaymeh@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "jaymeh/filament-dynamic-builder": "^1.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0 | ^11.0",
        "creode/laravel-repository": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Jaymeh\\FilamentPages\\": "src/",
            "Jaymeh\\FilamentPages\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jaymeh\\FilamentPages\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaymeh\\FilamentPages\\FilamentPagesServiceProvider"
            ],
            "aliases": {
                "FilamentPages": "Jaymeh\\FilamentPages\\Facades\\FilamentPages"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
